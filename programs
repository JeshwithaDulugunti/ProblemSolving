loops.js
for(let i=2;i<=8;i=i+2){
   if(i>4){
      break;
   }
   console.log(i+1)
}

for(let i=1;i<=8;i=i+2){
    if(i>5){
        continue
    }
    console.log(i+1)
}
OUTPUT: 2,4,6


for(let i = 2;i<=8;i=i+1){
    if(i>5){
        continue
    }
    console.log(i+2)
}
OUTPUT: 4, 5, 6, 7

for(let i=1;i<=3;i++){
    // document.write(i," ");
for(let j=i; j<=4; j++){
    document.write(j," ");
}
document.write(" ")
 }
